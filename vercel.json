
{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/manga/fetchId", "dest": "/api/manga/fetchId.js" },
    { "src": "/api/manga/search", "dest": "/api/manga/search.js" },
    { "src": "/api/manga/(?<id>[^/]+)", "dest": "/api/manga/[id].js" }
  ]
}
